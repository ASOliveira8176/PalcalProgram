//Título do Programa
Program Baskahra;
//Variáveis
Var
	 a,b,c,delta,x1,x2,x:real;
//Entrada de Dados
Begin
	write('Digite o valor de a: ');readln(a);
	write('Digite o valor de b: ');readln(b);
	write('Agora digite o valor de c: ');readln(c);
//Execução dos cálculos
		delta:=(b*b)-4*a*c;
	write('Este é o valor de delta: ',delta);
end.
